{
  "name": "deluge-rpc-socket",
  "version": "0.2.0",
  "description": "Node.js API for Deluge's Binary Socket RPC API",
  "main": "DelugeRPC.js",
  "scripts": {
    "format": "prettier --write \"**/*.@(js?(on)|md|prettierrc|clang-format|yml)\"",
    "preversion": "yarn format",
    "postversion": "npm publish",
    "postpublish": "git push origin master --tags",
    "start": "node --experimental-repl-await repl.js",
    "setup": "yarn && yarn mkconfig && install-vscode-extensions",
    "mkconfig": "echo Making new config.js for testing purposes. Please update it manually. && cp config.sample.js config.js"
  },
  "files": [
    "DelugeRPC.js"
  ],
  "repository": "git@github.com:cinderblock/node-deluge-rpc.git",
  "author": "Cameron Tacklind <cameron@tacklind.com>",
  "license": "MIT",
  "dependencies": {
    "pako": "^1.0.6",
    "python-rencode": "^1.1.2"
  },
  "devDependencies": {
    "install-vscode-extensions": "^1.0.0",
    "prettier": "^1.15.2",
    "repl-live": "^0.1.0",
    "yarn": "^1.12.3"
  }
}
